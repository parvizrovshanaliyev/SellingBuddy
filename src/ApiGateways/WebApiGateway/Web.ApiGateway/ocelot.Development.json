  "SwaggerEndPoints": [
    {
      "Key": "auth-key",
      "TransformByOcelotConfig": true,
      "Config": [
        {
          "Name": "Auth.API",
          "Version": "1.0",
          "Url": "http://localhost:5001/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "order-key",
      "TransformByOcelotConfig": true,
